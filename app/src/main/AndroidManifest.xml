<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.starbuzz">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".CartActivity"></activity>
        <activity android:name=".OrderActivity" />
        <activity android:name=".OutletActivity" />
        <activity android:name=".MenuDetailActivity" />
        <activity android:name=".MenuByCategoryActivity" />
        <activity android:name=".IdentityActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".CreateAccActivity" />
        <activity android:name=".WelcomeActivity" />
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>